{
    "version": "0.2.0",
    "configurations": [
      {
        "command": "yarn start",
        "name": "Run yarn start",
        "request": "launch",
        "type": "node-terminal"
      },
      {
        "name": "Debug Main Process",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
        "windows": {
          "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
        },
        "args" : ["."],
        "outputCapture": "std"
      }
    ]
  }